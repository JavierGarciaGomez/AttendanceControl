<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.ReviewRegistersController">
    <center>
        <TableView fx:id="tblTable" prefWidth="500">
            <columns>
                <TableColumn prefWidth="${tblTable.width*0.1}" text="ID"/>
                <TableColumn prefWidth="${tblTable.width*0.1}" text="UserName"/>
                <TableColumn prefWidth="${tblTable.width*0.1}" text="Branch"/>
                <TableColumn prefWidth="${tblTable.width*0.1}" text="Action"/>
                <TableColumn prefWidth="${tblTable.width*0.4}" text="Time"/>
                <TableColumn prefWidth="${tblTable.width*0.2}" text="Status"/>
            </columns>
        </TableView>
    </center>
</BorderPane>
