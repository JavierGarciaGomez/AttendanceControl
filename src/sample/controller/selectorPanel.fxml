<?import javafx.scene.text.Font?>

<GridPane alignment="CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllerer">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label alignment="TOP_CENTER" minWidth="-Infinity" text="Select the activity" GridPane.columnSpan="2" GridPane.halignment="CENTER">
         <font>
            <Font size="15.0" />
         </font></Label>
      <Button alignment="TOP_CENTER" minWidth="-Infinity" mnemonicParsing="false" prefWidth="100.0" text="Register" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button alignment="TOP_CENTER" minWidth="-Infinity" mnemonicParsing="false" prefWidth="100.0" text="Review" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button alignment="TOP_CENTER" minWidth="-Infinity" mnemonicParsing="false" prefWidth="100.0" text="Change" GridPane.halignment="CENTER" GridPane.rowIndex="2">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button alignment="TOP_CENTER" minWidth="-Infinity" mnemonicParsing="false" prefWidth="100.0" text="Incidences" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2">
